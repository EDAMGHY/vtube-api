<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Videos</title>
  </head>
  <body>
    <h1>Videos</h1>
    <% videos.forEach(function(video){ %>
    <video
      width="500"
      height="300"
      controls
      src="http://localhost:5000/<%= video.videoPath %>"
    ></video>
    <% }); %>
  </body>
</html>
